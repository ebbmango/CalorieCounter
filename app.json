{
  "expo": {
    "name": "CaloriesCounter",
    "slug": "CaloriesCounter",
    "version": "1.0.0",
    "web": {},
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-sqlite"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.CaloriesCounter"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.CaloriesCounter"
    }
  }
}
